CC = gcc
SRC = holberton.c main.c
NAME = holberton
OBJ = $(SRC:.c=.o)
RM = rm -f

all  : $(OBJ) m.h 
	$(CC) $(OBJ) -o $(NAME)
clean  :
	$(RM) *~ .*.sw* $(NAME)
oclean  :
	$(RM) $(OBJ)
fclean  :
	$(RM) *~ $(OBJ) $(NAME)
re  : oclean all
main.o : main.c
	gcc -c -o main.o main.c
holberton.o : holberton.c
	gcc -c -o holberton.o holberton.c
